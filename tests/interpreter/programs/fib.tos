fn fib_rec(i : Int) -> Int
{
	if i < 2
	{
		return i;
	}
	
	return fib_rec(i - 1) + fib_rec(i - 2);
}

fn fib_seq(i : Int) -> Int
{
	var cpt : Int = 1;
	var fib_1 : Int = 1;
	var fib_2 : Int = 0;
	var temp: Int;

	while cpt < i
	{
		temp = fib_1 + fib_2;
		fib_2 = fib_1;
		fib_1 = temp;
	}
	
	return fib_1;
}

fn main() -> Void
{
	print fib_rec(10);
	print fib_seq(10);
	
	return;
}
